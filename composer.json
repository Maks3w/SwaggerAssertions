{
    "name": "fr3d/swagger-assertions",
    "description": "Test your API requests and responses against your swagger definition",
    "keywords": [
        "api",
        "assertion",
        "phpunit",
        "swagger",
        "test",
        "testing"
    ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Maks3w",
            "email": "github.maks3w@virtualplanets.net"
        }
    ],
    "homepage": "https://github.com/Maks3w/SwaggerAssertions",
    "require": {
        "php": ">= 5.4.0",
        "justinrainbow/json-schema": "^1.4.2",
        "phpunit/phpunit": "~4.0",
        "rize/uri-template": "^0.3.0",
        "zendframework/zend-http": "~2.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.6",
        "guzzlehttp/guzzle": "~6.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "For Guzzle constraints"
    },
    "autoload": {
        "psr-4": {
            "FR3D\\SwaggerAssertions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FR3D\\SwaggerAssertionsTest\\": "tests/"
        }
    }
}
